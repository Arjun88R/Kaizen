{
  "manifest_version": 3,
  "name": "AI Job Tracker",
  "version": "0.1",
  "description": "A simple extension to track job applications.",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "https://us-central1-jacker-c269b.cloudfunctions.net/*",
    "https://jacker-c269b.web.app/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}